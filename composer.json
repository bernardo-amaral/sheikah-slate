{
    "name": "bernardo-amaral/sheikah-slate",
    "description": "Sheikah Slate Test Functions Library",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2"
    },
    "type": "library",
    "keywords": ["packagist", "tests"],
    "homepage": "https://github.com/bernardo-amaral/sheikah-slate",
    "license": "MIT",
    "authors": [
        {
            "name": "Bernardo Amaral",
            "email": "bernardo.amaral85@gmail.com",
            "homepage": "http://bernardo-amaral.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": { "bernardoamaral\\sheikah\\": "src/" }
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php --testdox tests/ --colors=always"
    }
}
